{
    "compilerOptions": {
      "moduleResolution": "node",
      "target": "es6",
      "module": "commonjs",
      "outDir": "./dist",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true
    },
    "include": [
      "src/**/*.ts",
      "env.d.ts"
    ],
    "exclude": ["node_modules"]
}